{
  "name": "financial-transactions-info",
  "version": "1.0.0",
  "description": "Implement a system that analyses financial transaction records",
  "main": "index.js",
  "repository": "localhost",
  "author": "ARUPPIN",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --open",
    "test": "jest --coverage",
    "format": "prettier --write \"src/**/*.js\"",
    "updatesnapshot": "yarn  test -- --updateSnapshot",
    "eslint": "standard --fix"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.29.4",
    "papaparse": "^5.1.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.9.0",
    "reactstrap": "^8.0.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^5.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "jest": "^24.9.0",
    "standard": "9.0.1",
    "webpack-dev-server": "^3.8.1"
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "/npm-packages-cache/",
      "/public/",
      "/styles/",
      "/dist"
    ],
    "globals": [
      "it",
      "test",
      "afterAll",
      "console",
      "protractor",
      "browser",
      "by",
      "element",
      "expect",
      "jest",
      "describe",
      "assert",
      "beforeEach",
      "afterEach",
      "fetch",
      "TextDecoder"
    ],
    "parser": "babel-eslint"
  }
}
